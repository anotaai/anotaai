--recuperar arquivos est√°ticos classloader
URL classesPath = null;
URL url = null;
List<Class<?>> classes = CPScanner.scanClasses(new ClassFilter().packageName("br.com.alinesolutions.anotaai.*").annotation(EnumSerialize.class));
ServletContext servletContext = sce.getServletContext();
Set<String> libJars = servletContext.getResourcePaths("/WEB-INF/classes");
for (String jar : libJars) {
	try {
		url = servletContext.getResource(jar);
		String urlString = url.toString();
		int index = urlString.lastIndexOf("/WEB-INF/classes/");
		urlString = urlString.substring(0, index + "/WEB-INF/classes/".length());
		classesPath = new URL(urlString);
		System.out.println(classesPath);
		break;
	} catch (MalformedURLException e) {
		throw new RuntimeException(e);
	}
}